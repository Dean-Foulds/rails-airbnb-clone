<div class="booking-wrapper">
  <div class="booking-img">
    <div class="container">
      <div class="row">
        <h3>Bookings:</h3>
        <div class="container">



          <% @homes.each do |home| %>

          <div class="row">
            <% home.bookings.each do |booking| %>
            <div class="col-sm-6 col-lg-3 ">
              <ul>
                <li>
                  <%= booking.starts_at %> at <%= booking.starts_at_time.strftime('%R') %> <br>
                  <%= booking.home.address %>
                </li>
              </ul>
            </div>
            <% end %>
            <%= link_to "Back", homes_path, class: "btn btn-default" %>
          </div>

          <% end %>



        </div>
      </div>
    </div>
  </div>
</div>
