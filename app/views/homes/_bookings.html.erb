  <table class="table table-striped table-hover">
    <thead>
    <tr>
        <th>Date</th>
        <th>Time</th>
        <th>Name</th>
        <th>Company</th>
      </tr>
    </thead>
    <tbody class="">


<% bookings.each do |booking| %>

      <tr>
        <td><%= booking.starts_at %></td>
        <td><%= booking.starts_at_time.strftime('%R') %></td>
        <td><%= current_user.email %></td>
        <td><%= current_user.company %></td>
      </tr>
      <% end %>
    </tbody>
  </table>


