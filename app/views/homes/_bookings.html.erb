  <table class="table table-striped table-hover">
    <thead>
    <tr>
        <th>Date</th>
        <th>Time</th>
        <th>Name</th>
        <th>Company</th>
        <th>Actions</th>

      </tr>
    </thead>
    <tbody class="">
      <% bookings.each do |booking| %> <!-- iternation -->
      <tr>
        <td><%= booking.starts_at %></td>
        <td><%= booking.starts_at_time.strftime('%R') %></td>
        <td><%= current_user.email %></td>
        <td><%= current_user.company %></td>
        <td><% if current_user %>
       <%= link_to "Delete", booking_path(booking), :class => "btn btn-default btn-sm",
       method: :delete,
       data: { confirm: "Are you sure?" } %>
       <% end %>
       </td>

      </tr>
      <% end %> <!-- iternation end -->


    </tbody>
  </table>





